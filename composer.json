{
    "name": "mckltech/lms-contracts",
    "description": "Shared contracts and interfaces for LMS provider packages (Thinkific, Kajabi, etc.)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Colin Longworth"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.0|^3.0",
        "saloonphp/pagination-plugin": "^2.2",
        "saloonphp/rate-limit-plugin": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "WooNinja\\LMSContracts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WooNinja\\LMSContracts\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
